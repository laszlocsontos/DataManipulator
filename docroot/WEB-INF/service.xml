<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">
<service-builder package-path="com.liferay.tool.datamanipulator">
	<author>Tibor Kov√°cs</author>
	<namespace>DM</namespace>

	<entity name="DataManipulator" local-service="true" remote-service="false" >

		<column name="id" type="long" primary="true" />

		<column name="groupId" type="long" />

		<column name="className" type="String" />

		<column name="classPK" type="long" />

		<finder name="ClassName" return-type="Collection" >
			<finder-column name="className" />
		</finder>
		<finder name="GroupId" return-type="Collection" >
			<finder-column name="groupId" />
		</finder>
		<finder name="C_C" return-type="DataManipulator" >
			<finder-column name="className" />
			<finder-column name="classPK" />
		</finder>
		<finder name="G_C" return-type="Collection" >
			<finder-column name="groupId" />
			<finder-column name="className" />
		</finder>
	</entity>
</service-builder>